<configuration>
  <appSettings>
    <!-- Keep AppID and Password fields blank to run in the Bot Emulator to test 1:1 bot functionality only  -->
    <!-- this is the default manifest's app id: -->
    <add key="TeamsAppId" value="" />
    <add key="MicrosoftAppId" value="" />
    <add key="MicrosoftAppPassword" value="" />
    <!-- To run this within Teams, add your registered Bot App Id and password below. 
    <add key="TeamsAppId" value="PASTE_YOUR_TEAMS_APP_ID_HERE"/>
    <add key="MicrosoftAppId" value="PASTE_YOUR_BOT_APPID_HERE" />
    <add key="MicrosoftAppPassword" value="PASTE_YOUR_BOT_APP_PASSWORD_HERE" />
    -->
    <!-- In Order to test connecto please specify the base URL and Connector Id below.
         Visit https://msdn.microsoft.com/en-us/microsoft-teams/connectors to create connector.
    -->
    <add key="ConnectorAppId" value="5a3d4797-0785-401d-b4d0-7f84b7dd2202" />
    <add key="BaseUrl" value="https://3098a01e.ngrok.io" />

    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <customErrors mode="Off" />
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6" />
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear />
        <add value="default.htm" />
      </files>
    </defaultDocument>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
